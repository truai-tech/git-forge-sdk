{
  "name": "git-forge",
  "version": "0.1.0",
  "description": "Unified TypeScript SDK for GitHub, GitLab, and Azure DevOps - manage branches and pull requests across providers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "git",
    "github",
    "gitlab",
    "azure-devops",
    "pull-request",
    "branch",
    "sdk",
    "effect"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "effect": "^3.12.0",
    "@octokit/rest": "^21.0.0",
    "@gitbeaker/rest": "^42.5.0",
    "azure-devops-node-api": "^14.1.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "dotenv": "^16.4.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.17.0",
    "@eslint/js": "^9.17.0",
    "typescript-eslint": "^8.18.0"
  }
}
